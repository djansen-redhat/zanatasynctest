msgctxt "c369822d9f842bda92409a4bb9690c38"
msgid "people could invent; chiefly carved woods of wonderful devices,"
msgstr "people could invent; chiefly carved woods of wonderful devices,"

msgctxt "a0ad2cdd0b9305bed3b8472f2953d5ea"
msgid "chiselled shells, inlaid spears, costly paddles, aromatic canoes;"
msgstr "chiselled shells, inlaid spears, costly paddles, aromatic canoes;"

msgctxt "731109074da2cfab4f27a43a13918c84"
