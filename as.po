msgstr "     BALEINE,             FRENCH."

msgctxt "a275adb92ff41af4462ef22d6528a3ba"
msgid "     BALLENA,             SPANISH."
msgstr "     BALLENA,             SPANISH."

msgctxt "21bfedea14a31975c287b3913d5252f3"
msgid "     PEKEE-NUEE-NUEE,     FEGEE."
msgstr "     PEKEE-NUEE-NUEE,     FEGEE."

