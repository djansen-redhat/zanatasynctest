msgstr "     HWAL,                SWEDISH."

msgctxt "109c144d4f019ab73fde1ba675928deb"
msgid "     WHALE,               ICELANDIC."
msgstr "     WHALE,               ICELANDIC."

msgctxt "ef15c98313064889159dce25cffb00cd"
